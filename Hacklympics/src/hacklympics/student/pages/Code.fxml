<AnchorPane id="AnchorPane" prefHeight="674.0" prefWidth="776.0" styleClass="mainFxmlClass" stylesheets="@../../../resources/Default.css" xmlns="http://javafx.com/javafx/8.0.162" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hacklympics.student.pages.CodeController">
   <children>
      <HBox prefHeight="674.0" prefWidth="776.0">
         <children>
            <VBox prefHeight="671.0" prefWidth="776.0" style="-fx-background-color: #f4f4f4;">
               <children>
                  <MenuBar prefHeight="29.0" prefWidth="776.0" style="-fx-background-color: #f5f6f7;">
                    <menus>
                      <Menu mnemonicParsing="false" text="File">
                        <items>
                          <MenuItem mnemonicParsing="false" onAction="#newFile" text="New " />
                              <MenuItem mnemonicParsing="false" text="Open..." />
                              <MenuItem mnemonicParsing="false" text="Save" />
                              <MenuItem mnemonicParsing="false" onAction="#saveFile" text="Save As..." />
                              <MenuItem mnemonicParsing="false" text="Close" />
                        </items>
                      </Menu>
                        <Menu mnemonicParsing="false" text="Code">
                           <items>
                              <MenuItem mnemonicParsing="false" onAction="#compile" text="Compile" />
                              <MenuItem mnemonicParsing="false" onAction="#execute" text="Run" />
                           </items>
                        </Menu>
                      <Menu mnemonicParsing="false" text="Exam">
                        <items>
                          <MenuItem mnemonicParsing="false" onAction="#submit" text="Submit" />
                        </items>
                      </Menu>
                      <Menu mnemonicParsing="false" text="Help">
                        <items>
                          <MenuItem mnemonicParsing="false" text="About" />
                        </items>
                      </Menu>
                    </menus>
                     <VBox.margin>
                        <Insets left="-3.0" />
                     </VBox.margin>
                  </MenuBar>
                  <TabPane fx:id="codeTabPane" prefHeight="756.0" prefWidth="776.0" tabClosingPolicy="ALL_TABS">
                    <tabs>
                      <Tab styleClass="file-tab" text="Code">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="542.0" prefWidth="776.0" styleClass="code-anchor">
                                 <children>
                                    <VBox prefHeight="605.0" prefWidth="776.0" styleClass="code-vbox">
                                       <children>
                                          <CodeArea fx:id="codeArea" onKeyPressed="#markAsUnsaved" prefHeight="594.0" prefWidth="766.0" styleClass="codearea" stylesheets="@../../../resources/JavaKeywords.css">
                                             <opaqueInsets>
                                                <Insets />
                                             </opaqueInsets>
                                             <VBox.margin>
                                                <Insets />
                                             </VBox.margin>
                                          </CodeArea>
                                       </children>
                                    </VBox>
                                 </children>
                              </AnchorPane>
                        </content>
                      </Tab>
                    </tabs>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets top="-7.0" />
                     </padding>
                  </TabPane>
               </children>
               <HBox.margin>
                  <Insets top="7.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
      <JFXTabPane fx:id="terminalPane" layoutY="467.0" mouseTransparent="true" opacity="0.0" prefHeight="207.0" prefWidth="776.0" style="-fx-background-color: #f4f4f4" tabClosingPolicy="UNAVAILABLE">
         <padding>
            <Insets bottom="10.0" />
         </padding>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </JFXTabPane>
      <HBox alignment="CENTER_RIGHT" layoutX="327.0" layoutY="25.0" prefHeight="52.0" prefWidth="449.0">
         <children>
            <Label fx:id="examLabel" alignment="CENTER_RIGHT" prefHeight="29.0" text="No Exam Selected">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets right="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <JFXComboBox fx:id="problemBox" minHeight="-Infinity" prefHeight="21.0" prefWidth="161.0" promptText="Pick a problem ...">
               <padding>
                  <Insets right="10.0" top="-3.0" />
               </padding>
            </JFXComboBox>
            <JFXButton onAction="#showHint" prefHeight="26.0" prefWidth="55.0" styleClass="darkBlueBtn" text="Hint">
               <HBox.margin>
                  <Insets left="10.0" right="15.0" />
               </HBox.margin>
            </JFXButton>
         </children>
      </HBox>
      <StackPane fx:id="dialogPane" layoutX="154.0" layoutY="237.0" prefHeight="123.0" prefWidth="291.0" />
   </children>
</AnchorPane>
